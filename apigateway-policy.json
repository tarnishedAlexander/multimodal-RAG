{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "apigateway:GET",
                "apigateway:POST",
                "apigateway:PUT",
                "apigateway:DELETE",
                "apigateway:PATCH"
            ],
            "Resource": "arn:aws:apigateway:us-east-2::*"
        },
        {
            "Effect": "Allow",
            "Action": "lambda:AddPermission",
            "Resource": "*"
        }
    ]
}
